<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Properties" sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <well-multifield jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well" fieldLabel="Nav Menus">
                        <items jcr:primaryType="nt:unstructured">
                            <navIcons jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldDescription="Nav Dialog" fieldLabel="Nav Menus" required="{Boolean}false" name="navIcons">
                                <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" emptyText="Nav Menu" name="./navIcons">
                                    <items jcr:primaryType="nt:unstructured">
                                        <label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="label" name="./label" />
                                        <dropdown jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Dropdown" name="./icon">
                                            <items jcr:primaryType="nt:unstructured">
                                                <message jcr:primaryType="nt:unstructured" text="message icon" value="icon-contattaci2" />
                                                <calendar jcr:primaryType="nt:unstructured" text="calendar icon" value="icon-calendar" />
                                                <globe jcr:primaryType="nt:unstructured" text="globe icon" value="icon-estero2" />
                                                <search jcr:primaryType="nt:unstructured" text="search icon" value="icon-search" />
                                            </items>
                                        </dropdown>
                                    </items>
                                </field>
                            </navIcons>
                        </items>
                    </well-multifield>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
