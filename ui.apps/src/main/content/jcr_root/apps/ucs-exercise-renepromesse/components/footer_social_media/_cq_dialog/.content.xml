<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Properties" sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <well-multifield jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well" fieldLabel="Social Media Links">
                        <items jcr:primaryType="nt:unstructured">
                            <navIcons jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldDescription="Nav Dialog" fieldLabel="Social media Links" required="{Boolean}false" name="navIcons">
                                <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" emptyText="Social Media links" name="./navIcons">
                                    <items jcr:primaryType="nt:unstructured">
                                        <url jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Url" name="./socialLink" />
                                        <dropdown jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Select the icon" name="./socialIcon">
                                            <items jcr:primaryType="nt:unstructured">
                                                <yt jcr:primaryType="nt:unstructured" text="Youtube" value="icon-youtube" />
                                                <twitter jcr:primaryType="nt:unstructured" text="Twitter" value="icon-twitter" />
                                                <linkedin jcr:primaryType="nt:unstructured" text="Linkedin" value="icon-linkedin" />
                                                <fb jcr:primaryType="nt:unstructured" text="Facebook" value="icon-facebook" />
                                                <ig jcr:primaryType="nt:unstructured" text="Instagram" value="icon-instagram" />
                                            </items>
                                        </dropdown>
                                        <target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Select the icon" name="./socialTarget">
                                            <items jcr:primaryType="nt:unstructured">
                                                <blank jcr:primaryType="nt:unstructured" text="Blank" value="_blank" />
                                                <self jcr:primaryType="nt:unstructured" text="Self" value="_self" />
                                             </items>
                                        </target>
                                    </items>
                                </field>
                            </navIcons>
                        </items>
                    </well-multifield>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
